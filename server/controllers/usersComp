// controllers/Controller.js
const usersComp = require('../models/UsersComp');

async function getAllUsersRecordsCont(req, res) {
  try {


    const records = await usersComp.getAllUsersRecords();

    //console.log(records.toString);

    res.json(records);


  } catch (error) {
    console.error('Error in getAllUsersComp controller:', error);
    res.status(500).json({ error: 'Server error' });
  }
}
 
module.exports = {
  getAllUsersRecordsCont,
};
 